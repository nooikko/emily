# Emily AI Agent - Environment Configuration Example
# Copy this file to .env and fill in your actual values

# ==========================================
# Core Application Configuration
# ==========================================
# NOTE: LOG_LEVEL is now stored in database with default: info
NODE_ENV=development
PORT=3000

# ==========================================
# Infisical Secret Management Configuration
# ==========================================
INFISICAL_ENABLED=true

INFISICAL_CLIENT_ID=b5198cfc-5dbe-4563-9ae2-4242be0a4e5f
INFISICAL_CLIENT_SECRET=7ac66ed0b9a7f5efa4dd1ec48dd3d7a32867ac2cfe2f8b3add85d2d998167caa
#INFISICAL_SERVICE_TOKEN=st.757d7157-e31d-46af-b5f9-f8fa6605f556.a37076dab8f733540c81a3494f7e7067.aed152f47981ab75f111ddd56211bc02

INFISICAL_PROJECT_ID=c0c24497-d833-43f9-95a2-d917f158f6a9
INFISICAL_ENVIRONMENT=dev
INFISICAL_FALLBACK_TO_ENV=true

# ==========================================
# LangSmith Configuration (Optional)
# ==========================================
LANGSMITH_ENABLED=false
LANGSMITH_TRACING=true

# ==========================================
# Unleash Configuration
# ==========================================
UNLEASH_ENABLED=true
UNLEASH_URL=http://localhost:4242/api
UNLEASH_APP_NAME=default
UNLEASH_ENVIRONMENT=development
UNLEASH_INSTANCE_ID=b625f403-207d-4c89-90a5-23dcbdf30932
UNLEASH_REFRESH_INTERVAL=15000
UNLEASH_METRICS_INTERVAL=60000
UNLEASH_CACHE_TTL=300000
UNLEASH_FALLBACK_TO_ENV=true
UNLEASH_TIMEOUT=10000
UNLEASH_RETRIES=2


# ==========================================
# ElevenLabs Configuration (Optional)
# ==========================================
ELEVENLABS_ENABLED=false

# ==========================================
# Database Configuration (PostgreSQL)
// Managed via Unleash (host, port, db) and Infisical (username, password)
// Do not set DB variables here to avoid drift

# ==========================================
# Vector Database Configuration (Qdrant)
# ==========================================
# Required for vector storage - no database defaults available
QDRANT_URL=http://localhost
QDRANT_PORT=6333
QDRANT_API_KEY=
QDRANT_COLLECTION_NAME=agent_memory

# ==========================================
# Memory Configuration
# ==========================================
# NOTE: ENABLE_SEMANTIC_MEMORY, MEMORY_RETRIEVAL_THRESHOLD, and MAX_MESSAGES_FOR_MEMORY
# are now stored in database with defaults: true, 0.7, 50
# Only MEMORY_BATCH_SIZE remains as environment variable

MEMORY_BATCH_SIZE=5

# ==========================================
# Embeddings Configuration
# ==========================================
# BGE Embeddings Model (alternative)
BGE_MODEL_NAME=BAAI/bge-base-en-v1.5

# ==========================================
# Development & Debugging
# ==========================================
# NOTE: DEBUG and DEV_MODE are now stored in database with defaults: false, true
# These can be managed through the configuration system

# ==========================================
# Production Deployment Notes
# ==========================================

# For production deployment:
# 1. Set NODE_ENV=production
# 2. Set LANGSMITH_HIDE_INPUTS=true
# 3. Set LANGSMITH_HIDE_OUTPUTS=true
# 4. Use strong passwords for all database connections
# 5. Enable SSL/TLS for database connections
# 6. Consider using separate LangChain projects for different environments
# 7. Set LANGCHAIN_CALLBACKS_BACKGROUND=false for serverless deployments

# ==========================================
# Self-Hosted LangSmith Configuration (Enterprise)
# ==========================================

# Uncomment and configure these if using self-hosted LangSmith
# LANGSMITH_LICENSE_KEY=your_enterprise_license_key
# LANGSMITH_SECRET_KEY=your_generated_secret_key
# POSTGRES_PASSWORD_LANGSMITH=secure_postgres_password_for_langsmith
# DATABASE_URL_LANGSMITH=postgresql://langsmith:secure_password@localhost:5432/langsmith
# REDIS_URL_LANGSMITH=redis://localhost:6379